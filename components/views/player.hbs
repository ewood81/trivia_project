<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <title>Tribage</title>
    {{> includes}}
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <script type="text/javascript" src="/js/PlayerClient.js"></script>
  </head>
  <body>
    {{> nav}}
    <div class="container fadeable">
      {{> user}}
      <div class="row">
        <div class="questionBody">
          {{> qa}}
        </div>
      </div>
      <div class="row">
        <div class="flex split">
          <button id="answer_submit_button"
            type="button"
            class="btn-primary"
            disabled="true">
            Submit
          </button>
          <div class="eventlabel infoDisplay">
            <span id="eventLabelEvent"></span>
            <span id="eventLabelTime"></span>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
