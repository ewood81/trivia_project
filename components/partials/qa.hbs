<div class="questionBody">
  <p>{{question}}</p>
  <ul>
    {{#each answers}}
      <div>
        <input
          type="radio"
          class="answer"
          name="answer"
          value="{{this}}"
          data-question="{{../id}}">
        <span class="answerLabel">{{this}}</span>
        <br>
      </div>
    {{/each}}
  </ul>
</div>
